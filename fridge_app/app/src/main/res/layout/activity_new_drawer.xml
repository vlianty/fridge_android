<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NewDrawerActivity">

    <TextView
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="50dp"
        android:text="Drawer"
        android:textAlignment="center"
        android:textSize="40sp" />


    <Button
        android:id="@+id/editbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="1dp"
        android:backgroundTint="#111"
        android:text="edit" />

    <RelativeLayout
        android:id="@+id/rel1"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_below="@id/editbtn"
        android:layout_marginTop="5dp"
        android:paddingLeft="5dp">

        <RelativeLayout
            android:id="@+id/text1"
            android:layout_width="200dp"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/drawer00"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:backgroundTint="#ECC0C4"
                android:gravity="center"
                android:padding="10dp"
                android:text="1 vegetable"
                android:textColor="#4E5155"
                android:textSize="25sp"
                android:textStyle="bold" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/text1">

            <Button
                android:id="@+id/drawer01"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:backgroundTint="#ECC0C4"
                android:gravity="center"
                android:padding="10dp"
                android:text="2 vegetable"
                android:textColor="#4E5155"
                android:textSize="25sp"
                android:textStyle="bold" />

        </RelativeLayout>

    </RelativeLayout>

    <!--    end of row 1-->

    <RelativeLayout
        android:id="@+id/rel2"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_marginTop="310dp"
        android:paddingLeft="5dp">
        <RelativeLayout
            android:id="@+id/text3"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            >

            <Button
                android:id="@+id/drawer10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:backgroundTint="#ECC0C4"
                android:textColor="#4E5155"
                android:gravity="center"
                android:padding="10dp"
                android:text="2 seafood"
                android:textSize="25sp"
                android:textStyle="bold" />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/text3">

            <Button
                android:id="@+id/drawer11"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:backgroundTint="#ECC0C4"
                android:textColor="#4E5155"
                android:gravity="center"
                android:padding="10dp"
                android:text="3 seafood"
                android:textSize="25sp"
                android:textStyle="bold" />

        </RelativeLayout>
    </RelativeLayout>
    <!--    end of row 2-->

    <RelativeLayout
        android:id="@+id/rel3"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:layout_marginTop="450dp"
        android:paddingLeft="5dp">
        <RelativeLayout
            android:id="@+id/text5"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            >

            <Button
                android:id="@+id/drawer20"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:backgroundTint="#ECC0C4"
                android:textColor="#4E5155"
                android:gravity="center"
                android:padding="10dp"
                android:text="3 meat"
                android:textSize="25sp"
                android:textStyle="bold" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/text6"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/text5">

            <Button
                android:id="@+id/drawer21"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:layout_marginBottom="5dp"
                android:backgroundTint="#ECC0C4"
                android:textColor="#4E5155"
                android:gravity="center"
                android:padding="10dp"
                android:text="3 meat"
                android:textSize="25sp"
                android:textStyle="bold" />

        </RelativeLayout>

    </RelativeLayout>

    <Button
        android:id="@+id/button433"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="464dp"
        android:layout_toStartOf="@+id/editbtn"
        android:text="433" />

    <Button
        android:id="@+id/button846"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="465dp"
        android:layout_toStartOf="@+id/button433"
        android:text="846" />

    <Button
        android:id="@+id/button847"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="463dp"
        android:layout_toStartOf="@+id/button846"
        android:text="847" />

    <Button
        android:id="@+id/button432"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button847"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="304dp"
        android:layout_marginBottom="11dp"
        android:text="432" />


